{
  "name": "faultline",
  "version": "1.0.0",
  "description": "faultline: Error traking tool on AWS managed services.",
  "main": "index.js",
  "scripts": {
    "deploy": "npm install && sls deploy -v",
    "destroy": "sls remove",
    "info": "sls info",
    "test": "nyc --reporter=lcovonly mocha './src/**/__tests__/*.js' --require intelli-espower-loader",
    "report": "nyc report",
    "coverage": "nyc --reporter=text-lcov mocha './src/**/__tests__/*.js' | coveralls",
    "doc": "jdoc schema.json --markdown-template-path jdoc.template.md.erb > docs/api.md"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/lib/mockUtility.js",
      "src/**/__tests__/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/faultline/faultline.git"
  },
  "keywords": [
    "error-monitoring",
    "crash-reporting",
    "aws-lambda",
    "serverless",
    "notifications",
    "slack",
    "github"
  ],
  "author": "k1LoW <k1lowxb@gmail.com> (https://github.com/k1LoW)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/faultline/faultline/issues"
  },
  "homepage": "https://github.com/faultline/faultline#readme",
  "dependencies": {
    "ajv": "^6.1.0",
    "ajv-keywords": "^3.0.0",
    "axios": "^0.15.2",
    "bluebird": "^3.5.1",
    "github": "^7.0.0",
    "gitlab": "^1.7.1",
    "http-errors": "^1.6.2",
    "js-sha256": "^0.5.0",
    "js-yaml": "^3.10.0",
    "json-schema-deref-sync": "^0.3.4",
    "lodash": "^4.16.2",
    "middy": "^0.8.5",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.5.3",
    "nyc": "^11.4.1",
    "octopublish": "^0.5.0",
    "power-assert": "^1.4.3",
    "serverless": "^1.25.0",
    "serverless-prune-plugin": "^1.2.0"
  }
}
